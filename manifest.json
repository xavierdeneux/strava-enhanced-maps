{
  "name": "Strava Enhanced Maps",
  "version": "2.3.0",
  "manifest_version": 3,
  "description": "Extension Strava qui permet d'ajouter de nouveaux fonds de carte au planificateur d'itin√©raire.",
  "homepage_url": "https://github.com/xavierdeneux/strava-enhanced-maps",
  "icons": {
    "128": "icons/icon128.png"
  },
  "content_scripts": [{
    "matches": [
      "https://www.strava.com/*"
    ],
    "js": [
      "index.js"
    ]
  }],
  "web_accessible_resources": [{
    "resources": [
      "enhancedMaps.js",
      "lib/arrive.min.js",
      "lib/jquery-3.5.1.min.js",
      "lib/jquery-ui-1.3.1.min.js",
      "lib/jquery-ui-1.3.1.min.css",
      "style.css"
    ],
    "matches": ["<all_urls>"]
  }],
  "host_permissions": [
    "https://www.strava.com/*"
  ]
}