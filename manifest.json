{
  "name": "Strava Enhanced Maps",
  "version": "2.4.1",
  "manifest_version": 3,
  "description": "Extension Strava qui permet d'ajouter de nouveaux fonds de carte au planificateur d'itin√©raire.",
  "homepage_url": "https://github.com/xavierdeneux/strava-enhanced-maps",
  "icons": {
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.strava.com/*"],
      "js": ["index.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "enhancedMaps.js",
        "lib/tingle.min.js",
        "lib/tingle.min.css",
        "style.css"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "host_permissions": ["https://www.strava.com/*"],
  "browser_specific_settings": {
    "gecko": {
      "id": "{e94e143f-ee9f-4703-9085-0e9d94c6d962}"
    }
  }
}
